{
  "name": "fabzclean",
  "version": "1.0.0",
  "description": "FabZClean Enterprise Management System",
  "author": "Kavin",
  "main": "main.cjs",
  "type": "module",
  "license": "MIT",
  "files": [
    "main.cjs",
    "preload.cjs",
    "package.json",
    "node_modules/**/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "build": {
    "appId": "com.fabzclean.app",
    "productName": "FabZClean",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "directories": {
      "output": "dist-electron",
      "buildResources": "installer_assets"
    },
    "asar": true,
    "asarUnpack": [
      "dist-server/**/*",
      "node_modules/better-sqlite3/**/*",
      "node_modules/sharp/**/*"
    ],
    "mac": {
      "category": "public.app-category.business",
      "icon": "client/public/assets/fabclean-logo-512.png",
      "hardenedRuntime": true,
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "client/public/assets/fabclean-logo-512.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "FabZClean"
    },
    "linux": {
      "icon": "client/public/assets/fabclean-logo-512.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    },
    "files": [
      "main.cjs",
      "preload.cjs",
      "package.json",
      "dist/**/*",
      "dist-server/**/*"
    ],
    "extraResources": [
      {
        "from": "dist-server",
        "to": "dist-server",
        "filter": [
          "**/*"
        ]
      }
    ],
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "publish": null,
    "extraFiles": [
      {
        "from": "node_modules/better-sqlite3",
        "to": "node_modules/better-sqlite3",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/bindings",
        "to": "node_modules/bindings",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/file-uri-to-path",
        "to": "node_modules/file-uri-to-path",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ec2": "NODE_OPTIONS='--max-old-space-size=4096' npm run build",
    "build:client": "vite build --config vite.config.ts",
    "build:server": "esbuild server/minimal-server.ts --bundle --platform=node --target=node18 --outfile=dist-server/server.js --format=esm --packages=external",
    "postinstall": "npm run build:client",
    "start": "npx tsx server/minimal-server.ts",
    "start:prod": "NODE_ENV=production node dist-server/server.js",
    "start:client": "vite preview --port 5000",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:setup": "npx tsx server/local-db-setup.ts",
    "db:reset": "rm -rf server/secure_data/fabzclean.db && npx tsx server/local-db-setup.ts",
    "db:reset-password": "npx tsx server/reset-admin-password.ts",
    "db:fix-admin": "npx tsx server/fix-admin.ts",
    "code:cleanup": "node scripts/code-cleanup.js",
    "code:cleanup:fix": "node scripts/code-cleanup.js --fix",
    "code:cleanup:verbose": "node scripts/code-cleanup.js --verbose",
    "audit:security": "npm audit --audit-level=high",
    "dev:mac": "./scripts/dev-mac.sh",
    "check:dead-code": "./scripts/find-dead-code.sh",
    "check:console": "./scripts/check-console-log.sh",
    "db:check": "drizzle-kit push --check",
    "prepare": "husky",
    "test:e2e": "npx playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder -w"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
